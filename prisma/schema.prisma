generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./default.db"
}

model Assistant {
  id           Int      @id @default(autoincrement())
  name         String   @unique
  assistant_id String
  created_at   DateTime @default(now())
  updated_at   DateTime @updatedAt
}

model Thread {
  id         Int      @id @default(autoincrement())
  owner      String   @unique
  thread_id  String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
